<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WPLauncher.TileComponent">
  <ContentView.Content>
      <StackLayout>
            <BoxView x:Name="Tile" BindingContext="{x:Reference Tile}" HeightRequest="{Binding Width}"/>
        </StackLayout>
  </ContentView.Content>
</ContentView>